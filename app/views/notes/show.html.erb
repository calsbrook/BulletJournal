<% if flash[:notice] %>
    <div class="notice"><%= flash[:notice] %></div>
<% end %>
<h4><%= @note.due.month %>/<%= @note.due.day%>/<%=@note.due.year%></h4>


<p id="content"><%= @note.content %></p>

<%= link_to "Home", notes_path %> |
<%= link_to "Delete", note_path(@note), method: :delete, data: {confirm: "Are you sure you want to delete this note?"}%> |
<%= link_to "Edit", edit_note_path(@note) %>